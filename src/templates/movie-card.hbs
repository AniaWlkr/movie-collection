<li class="movie-item" id={{id}} data-sourse={{id}}>
  <img src="{{poster_path}}" alt="{{original_title}}" class="movie-img" data-create="poster" id={{id}}>
  <div class="movie-wrapper">
    <h3 class="movie-title" data-create="title">{{original_title}}</h3>
    <p class="movie-info" data-create="release-date"><span class="movie-genres" data-create="genres">{{genres}}</span> |
      <span class="movie-date">{{release_date}}
    </p>
  </div>
</li>