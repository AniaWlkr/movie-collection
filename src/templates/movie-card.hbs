{{#each this}}
<li class="movie-item">
  <img src="https://image.tmdb.org/t/p/w500{{poster_path}}" alt="{{original_title}}" class="movie-img">
  <div class="movie-wrapper">
    <h3 class="movie-title">{{original_title}}{{original_name}}</h3>
    <p class="movie-info"><span class="movie-genres">{{genre}}</span> | <span class="movie-date">{{release_date}}
    </p>
  </div>
</li>
{{/each}}